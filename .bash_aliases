alias a='alias'

a ll='ls -la'
a lt='ls -latnr'
a lld="ll | egrep '^d'"
a lsd='lld'
a ltd="lt | egrep '^d'"

a llg='ll | grep'
a ltg='lt | grep'

a ..='cd ..'
a ....='cd ../..'
a ......='cd ../../..'
a u='..'
a uu='u;u'
a uuu='u;u;u'

a c='clear'
a h='history'
a hig='h | grep'

a ps='ps -eaf'
a psg='ps | grep'

a gs='git status'

a fw='source ~/.bashrc'

a aliases='nano ~/.bash_aliases'
a bashrc='nano ~/.bashrc'
